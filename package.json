{
  "name": "boardgames",
  "version": "1.0.0",
  "description": "Api de busqueda de juegos de mesa",
  "main": "index.js",
  "scripts": {
    "start": "node index.js.js",
    "server": "nodemon index.js.js",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "concurrently \"npm run server\" \"npm run client\"",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SortegaG/boardgames.git"
  },
  "author": "Sergio Ortega",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SortegaG/boardgames/issues"
  },
  "homepage": "https://github.com/SortegaG/boardgames#readme",
  "dependencies": {
    "concurrently": "^9.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "pg": "^8.13.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
